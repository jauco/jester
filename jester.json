{
    "eslintRulesDir": "./eslint-rules/",
    "srcPath": "./src/",
    "entryGlob": "app/features/*/feature.js",
    "karmaPath": "./build/karma/",
    "artifactPath": "./build/artifacts",
    "karmaOptions": {
        "proxies": {},
        "browsers": [
            "Chrome",
            "Firefox",
            "IE",
            "PhantomJS"
        ]
    },
    "eslintRules": {
        "no-cond-assign": 2,
        "no-console": 0,
        "no-comma-dangle": 2,
        "no-control-regex": 2,
        "no-debugger": 1,
        "no-dupe-keys": 2,
        "no-empty": 2,
        "no-empty-class": 2,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-parens": 1,
        "no-extra-semi": 2,
        "no-func-assign": 2,
        "no-invalid-regexp": 2,
        "no-negated-in-lhs": 2,
        "no-obj-calls": 2,
        "no-regex-spaces": 2,
        "no-spare-arrays": 0,
        "no-unreachable": 2,
        "use-isnan": 2,
        "valid-jsdoc": 0,
        "block-scoped-var": 0,
        "complexity": [
            2,
            20
        ],
        "consistent-return": 2,
        "curly": [
            2,
            "all"
        ],
        "dot-notation": 0,
        "eqeqeq": 2,
        "guard-for-in": 2,
        "no-alert": 1,
        "no-caller": 2,
        "no-div-regex": 2,
        "no-else-return": 0,
        "no-empty-label": 2,
        "no-eq-null": 2,
        "no-eval": 2,
        "no-extend-native": 2,
        "no-fallthrough": 2,
        "no-floating-decimal": 2,
        "no-implied-eval": 2,
        "no-labels": 2,
        "no-iterator": 2,
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        "no-multi-str": 0,
        "no-native-reassign": 2,
        "no-new": 2,
        "no-new-func": 2,
        "no-new-wrappers": 2,
        "no-octal": 2,
        "no-octal-escape": 2,
        "no-proto": 2,
        "no-redeclare": 2,
        "no-return-assign": 2,
        "no-script-url": 2,
        "no-self-compare": 2,
        "no-unused-expressions": 2,
        "no-with": 2,
        "no-yoda": 2,
        "radix": 2,
        "wrap-iife": 2,
        "no-global-strict": 0,
        "no-extract-strict": 0,
        "strict": 0,
        "no-catch-shadow": 2,
        "no-delete-var": 2,
        "no-label-var": 2,
        "no-shadow": 2,
        "no-shadow-restricted-names": 2,
        "no-undef": 2,
        "no-undef-init": 2,
        "no-unused-vars": 1,
        "no-use-before-define": 2,
        "no-mixed-requires": 0,
        "no-path-concat": 2,
        "no-process-exit": 2,
        "no-sync": 1,
        "brace-style": 2,
        "camelcase": 2,
        "consistent-this": [
            2,
            "self"
        ],
        "func-names": 0,
        "func-style": [
            2,
            "declaration"
        ],
        "new-cap": 2,
        "new-parens": 2,
        "no-nested-ternary": 2,
        "no-array-constructor": 2,
        "no-new-object": 2,
        "no-spaced-func": 2,
        "no-ternary": 0,
        "no-underscore-dangle": 0,
        "no-wrap-func": 2,
        "quotes": [
            1,
            "double",
            "avoid-escape"
        ],
        "quote-props": 0,
        "semi": 2,
        "sort-vars": 0,
        "space-infix-ops": 2,
        "space-return-throw-case": 2,
        "space-unary-word-ops": 2,
        "max-nested-callbacks": [
            1,
            5
        ],
        "one-var": 0,
        "wrap-regex": 0,
        "max-depth": 0,
        "max-len": 0,
        "max-params": 0,
        "max-statements": 0,
        "no-bitwise": 1,
        "no-plusplus": 0
    }
}