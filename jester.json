{
    "eslintRulesDir": "./eslint-rules/",
    "srcPath": "./src/",
    "apiDocPath": "./doc/api/",
    "jsdocConf": "./jsdoc.conf",
    "readme": "./readme.md",
    "entryGlob": "app/features/*/feature.js",
    "karmaPath": "./build/karma/",
    "artifactPath": "./build/artifacts",
    "karmaOptions": {
        "basePath": "./build/karma/",
        "frameworks": ["jasmine"],
        "files": ["*.js"],
        "reporters": ["dots"],
        "proxies": {},
        "browsers": [
            "Chrome",
            "Firefox",
            "IE",
            "PhantomJS"
        ]
    }
}